<form action="/editarAvaliacao/{{avaliacao.id}}" method="post">
    <label for="nota1">Nota 1:</label><br>
    <input type="number" id="nota1" name="nota1" value="{{avaliacao.nota1}}" min="0" max="10" step="0.1" required><br>
    <label for="nota2">Nota 2:</label><br>
    <input type="number" id="nota2" name="nota2" value="{{avaliacao.nota2}}" min="0" max="10" step="0.1" required><br>
    <input type="submit" value="Salvar Avaliação">
</form>
